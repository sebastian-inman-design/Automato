{
  "name": "automato",
  "version": "1.0.0",
  "description": "Automated website testing",
  "author": {
    "name": "Sebastian Inman",
    "email": "hello@sebastianinman.me",
    "url": "https://www.sebastianinman.me"
  },
  "scripts": {
    "build:application": "tsc",
    "build:renderer": "vue-cli-service build",
    "build": "npm run build:application && npm run build:renderer",
    "watch:application": "tsc --watch",
    "watch:renderer": "vue-cli-service serve",
    "watch": "concurrently \"npm run watch:application\" \"npm run watch:renderer\"",
    "devtools": "./node_modules/.bin/vue-devtools",
    "start": "set NODE_ENV=production&&electron build/index.js",
    "dev": "set NODE_ENV=development&&electron build/index.js"
  },
  "dependencies": {
    "@vue/cli-service": "^4.5.9",
    "axios": "^0.21.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.4.9",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/electron": "^1.6.10",
    "@types/express": "^4.17.9",
    "@types/vue": "^2.0.0",
    "@vue/cli": "^4.5.9",
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-pwa": "^4.5.9",
    "@vue/cli-plugin-router": "^4.5.9",
    "@vue/cli-plugin-typescript": "^4.5.9",
    "@vue/cli-plugin-vuex": "^4.5.9",
    "@vue/devtools": "^5.3.3",
    "autoprefixer": "^9.8.6",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.1",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.6",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^10.1.0",
    "sass-resources-loader": "^2.0.3",
    "style-loader": "^2.0.0",
    "tailwindcss": "^1.6.2",
    "typescript": "^4.1.2",
    "vue-template-compiler": "^2.6.12"
  },
  "keywords": [],
  "license": "ISC"
}
